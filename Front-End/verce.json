{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://ticket-ai-backend.onrender.com/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate, proxy-revalidate"
        }
      ]
    }
  ],
  "devCommand": "vite",
  "buildCommand": "vite build",
  "outputDirectory": "dist"
}